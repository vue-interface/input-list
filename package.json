{
  "name": "@vue-interface/input-list",
  "version": "2.0.0-beta.0",
  "description": "A Vue input list component.",
  "files": [
    "dist",
    "tailwindcss"
  ],
  "type": "module",
  "main": "./dist/input-list.umd.cjs",
  "module": "./dist/input-list.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/input-list.js",
      "require": "./dist/input-list.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "browserslist": "last 2 versions, > 0.5%, ie >= 11",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "pre-release": "npm run build; git add . -A; git commit -m 'pre-release commit'",
    "release-patch": "npm run pre-release && npm version patch -m \"%s\" && npm run release;",
    "release-minor": "npm run pre-release && npm version minor -m \"%s\" && npm run release;",
    "release-major": "npm run pre-release && npm version major -m \"%s\" && npm run release;",
    "release": "git add . -A; git commit; git push --tags origin; npm publish;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-interface/input-list.git"
  },
  "keywords": [
    "Input",
    "List",
    "Field",
    "Vue",
    "Component",
    "Tailwind",
    "Bootstrap"
  ],
  "author": "Justin Kimbrell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vue-interface/input-list/issues"
  },
  "homepage": "https://github.com/vue-interface/input-list/docs#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.2",
    "change-case": "^4.1.2",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "pascalcase": "^2.0.0",
    "pinia": "^2.0.23",
    "postcss": "^8.4.6",
    "postcss-functions": "^4.0.2",
    "postcss-hexrgba": "^2.0.1",
    "tailwindcss": "^3.0.18",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.6.6",
    "vue": "^3.2.41"
  }
}
